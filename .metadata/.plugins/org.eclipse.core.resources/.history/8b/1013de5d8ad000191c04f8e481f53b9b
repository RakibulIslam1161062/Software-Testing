import static org.junit.jupiter.api.Assertions.*;

import java.time.LocalDate;
import java.time.Month;

import org.junit.jupiter.api.Test;

class BookFilterTEsting {

	@Test
	void afterCheck1() {
		LocalDate date = LocalDate.of(1900, Month.MAY, 7);
		Book newBook2 = new Book("Java Book", "rakib",date );
		BookFilter bookPublishedYearFilter = new BookPublishedYearFilter();
		Boolean check = bookPublishedYearFilter.apply(newBook2);
		System.out.println(check);
		
		
		assertArrayEquals(check,true);
		
		
	}

}
